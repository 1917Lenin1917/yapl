interface Printable {
  fn print(): str;
}

type Penis {
  a: str;
  b: int;
  c: float;
  d: Array;
}

impl Printable for Penis {
  fn print(): str {
    return `Penis: a={a}, b={b}, c={c}, d={d}`;
  }
}

const penis1 = Penis{ .a = "Hello, World!",
                     .b = 20,
                     .c = 40.0,
                     .d = [1, 2, 3, 4, 5] };

const penis2 = Penis{ "Test", 1, 2.3, [1,2,3] };

fn my_print(printable: Printable): void {
  print(printable.print());
}

my_print(penis1);
my_print(penis2);