const array = [5, 4, 3, 2, 1];

fn bubble_sort(arr: Array): Array {
  let i = 0, j = 0, temp = 0;
  while i < arr.size() {
    while j < arr.size() {
      if arr[i] < arr[j] {
        temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
      }
      j = j + 1;
    }
    i = i + 1;
    j = 0;
  }
  return arr;
}

print(bubble_sort(array));